{{>header }}

<form input="action" method="POST" style="padding-top:400px;background-color:#000000;padding-left:100px;">
  <label for="search">First name:</label><br>
  <input type="text" id="search" value="Search" style="background-color:#2e3131;color:white;width:700px;"><br><br>
  <input type="submit" value="Submit">
</form>
<div class='col-sm-12' style="background-color:  #2e3131;padding-bottom: 20px;">
  <div id="myCarousel" class="carousel slide" data-ride="carousel" style="width: 100%;padding-top: 0px;">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" style="width: 100%">

      <div class="item active">
          <div class="col-sm-4" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p1.jpg" style="height: 550px;width: 570px;"  style="width:100%;">
          </div>
          <div class="col-sm-4" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p2.jpg" style="height: 550px;width: 570px;"  style="width:100%;">
          </div>
          <div class="col-sm-4" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p3.jpg" style="height: 550px;width: 570px;"  style="width:100%;">
          </div>
      </div>

      <div class="item ">
          <div class="col-sm-4" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p4.jpg" style="height: 550px;width: 570px;"  style="width:100%;">
          </div>
          <div class="col-sm-4" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p10.jpg" style="height: 550px;width: 570px;"  style="width:100%;">
          </div>
          <div class="col-sm-4"style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p11.jpg" style="height: 550px;width: 570px;"  style="width:100%;">
          </div>
             
      </div>
       
      <div class="item">
            <div class="col-sm-6" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p7.jpg" style="height: 550px;width: 870px;"  style="width:100%;">
            </div>
            <div class="col-sm-6" style="padding-left: 0px;padding-right: 0px;">
            <img src="img/p8.jpg" style="height: 550px;width: 870px;"  style="width:100%;">
            </div>
      </div>
  
    </div> 

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
      <span class="sr-only">Next</span>
    </a>
</div>
</div>
{{>footer}}
